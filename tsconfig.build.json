{
  "compilerOptions": {
    "lib": [
      "es2017",
      "dom"
    ],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "declaration": false,
    "sourceMap": false,
    "jsx": "react",
    "baseUrl": "src",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
